{
  "name": "movie_app",
  "description": "Simple app, utilizing TheMovieDB.org's API, movies that are searched are stored in a local MongoDB collection. Users may register, favorite/rate/review movies.",
  "main": "app.js",
  "authors": [
    "Ed Petner"
  ],
  "license": "ISC",
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "public/lib",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.6.3"
  },
    "overrides": {
      "bootstrap": {
